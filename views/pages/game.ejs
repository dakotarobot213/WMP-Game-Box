		<nav>
			<h2>Memory Match</h2>
			<h4>Match each card with the same color and value!</h4>
			<h4 id="player-turn">Player 1's turn</h4>
			<img
				id="options-menu"
				src="./resources/icons8-hamburger-menu-128.png"
			/>
		</nav>
		<section id="options">
			<h1>Options</h1>
			<h3 id="player-score">Score: Make a match to create</h3>
			<h3>
				Players:
				<button
					onclick="setPlayerCount(1)"
					class="player-count-btn"
				>
					1</button
				><button
					onclick="setPlayerCount(2)"
					class="player-count-btn"
				>
					2</button
